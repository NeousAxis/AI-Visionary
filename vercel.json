{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "public"
            }
        },
        {
            "src": "api/config.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/config",
            "dest": "/api/config.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}